		},
		"tnRoutineWithNoNumber": {
			"enum": [
				null,
				"",
				"port-no-temp",
				"auto",
				"no-number"
			]
		},
		"bwksDnSchema": {
			"type": "string",
			"description": "A formatted TN.",
			"pattern": "^\\+\\d+-\\d+$",
			"minLength": 1,
			"maxLength": 23,
			"required": false
		},
